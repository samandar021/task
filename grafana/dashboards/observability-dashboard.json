  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests / sec",
      "id": 1,
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(rate(app_requests_total[1m]))",
          "legendFormat": "rps",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "id": 2,
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(app_request_latency_seconds_bucket[2m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "type": "timeseries",
      "title": "CPU usage %",
      "id": 3,
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[1m])) * 100)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "type": "timeseries",
      "title": "Memory usage %",
      "id": 4,
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "type": "stat",
      "title": "Errors Total",
      "id": 5,
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(app_errors_total)",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16}
    },
    {
      "type": "stat",
      "title": "Warnings Total",
      "id": 7,
      "datasource": {"type": "prometheus", "uid": "prometheus"},
      "targets": [
        {
          "expr": "sum(app_warnings_total)",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 16}
    },
    {
      "type": "logs",
      "title": "Application Logs",
      "id": 6,
      "datasource": {"type": "loki", "uid": "loki"},
      "targets": [
        {
          "expr": "{job=\"app\"}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    }
  ],
  "refresh": "10s",
